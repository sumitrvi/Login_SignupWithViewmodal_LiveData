<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto"
             android:id="@+id/nav_graph"
    app:startDestination="@id/loginfragment2">

    <fragment
        android:id="@+id/loginfragment2"
        android:name="com.notesapi.views.Loginfragment"
        android:label="Loginfragment" >
        <action
            android:id="@+id/action_loginfragment2_to_registartionFragment2"
            app:destination="@id/registartionFragment2" />
        <action
            android:id="@+id/action_loginfragment2_to_homeFragment2"
            app:destination="@id/homeFragment2" />
    </fragment>
    <fragment
        android:id="@+id/registartionFragment2"
        android:name="com.notesapi.views.RegistartionFragment"
        android:label="RegistartionFragment" >
        <action
            android:id="@+id/action_registartionFragment2_to_loginfragment2"
            app:destination="@id/loginfragment2" />
        <action
            android:id="@+id/action_registartionFragment2_to_homeFragment2"
            app:destination="@id/homeFragment2" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment2"
        android:name="com.notesapi.views.HomeFragment"
        android:label="HomeFragment" />
</navigation>